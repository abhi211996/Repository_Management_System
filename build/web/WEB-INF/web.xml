<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>additem</servlet-name>
        <servlet-class>additem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addvender</servlet-name>
        <servlet-class>addvender</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>complaint</servlet-name>
        <servlet-class>complaint</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchitem</servlet-name>
        <servlet-class>searchitem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchlocation</servlet-name>
        <servlet-class>searchlocation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchvender</servlet-name>
        <servlet-class>searchvender</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>additem</servlet-name>
        <url-pattern>/additem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addvender</servlet-name>
        <url-pattern>/addvender</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>complaint</servlet-name>
        <url-pattern>/complaint</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchitem</servlet-name>
        <url-pattern>/searchitem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchlocation</servlet-name>
        <url-pattern>/searchlocation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchvender</servlet-name>
        <url-pattern>/searchvender</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
